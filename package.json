  "bugs": {
    "url": "https://github.com/ijmccallum/ijmccallum.github.io/issues"
  },
  "homepage": "https://github.com/ijmccallum/ijmccallum.github.io#readme",
  "devDependencies": {
    "async": "^2.6.0",
    "autoprefixer": "^7.2.5",
    "cssnano": "^3.10.0",
    "mini-site-generator": "^1.0.8",
    "node-sass": "^4.7.2",
    "pa11y": "^4.13.2",
    "postcss": "^6.0.17",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.1.0"
  }
}
