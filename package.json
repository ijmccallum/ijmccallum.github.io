{
  "name": "ijmccallum.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run build:markup && npm run build:css",
    "build:markup": "msg -io ./src/ ./",
    "build:css": "npm run sass && npm run autoprefixer && npm run cssnano",
    "sass": "node-sass --output-style compressed ./src/scss/main.scss ./css/main.css",
    "autoprefixer": "postcss -u autoprefixer -r css/**/*.css --no-map",
    "cssnano": "postcss -u cssnano -r css/**/*.css --no-map",
    "postbuild": "node pa11y.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ijmccallum/ijmccallum.github.io.git"
  },
