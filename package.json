{
  "name": "ijmccallum.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "run-p start:server start:watch",
    "start:server": "http-server ./ -d",
    "start:watch": "node bin/watch.js",
    "build": "run-s build:css build:markup",
    "build:markup": "msg -io ./src/ ./",
    "build:css": "run-s build:css:postcss build:markup",
    "build:css:postcss": "postcss --use postcss-import postcss-custom-media autoprefixer cssnano -o css/main.css src/css/main.css --no-map",
    "test": "npm run test:a11y",
    "test:a11y": "node pa11y.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ijmccallum/ijmccallum.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ijmccallum/ijmccallum.github.io/issues"
  },
  "homepage": "https://github.com/ijmccallum/ijmccallum.github.io#readme",
  "devDependencies": {
    "async": "^2.6.0",
    "autoprefixer": "^7.2.5",
    "cssnano": "^3.10.0",
    "http-server": "^0.11.1",
    "mini-site-generator": "^1.0.10",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "pa11y": "^4.13.2",
    "postcss": "^6.0.17",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^11.1.0"
  }
}
